<h1>Management sign up</h1>
<% if @management.errors.any? %>
  <div id="error_explanation">
    <h2><%= @management.errors.count %>件のエラーがあります。</h2>
    <ul>
      <% @management.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>
<%= form_with(model: @management, local: true) do |f| %>

  <%= f.label :kid_id %>
  <%= f.text_field :kid_id %>

  <%= f.submit "Create my account" %>
<% end %>
